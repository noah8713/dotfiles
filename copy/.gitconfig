[branch]
    autosetupmerge = true
[core]
    ;  autocrlf = true
    ;  autocrlf = input
    ;  whitespace = cr-at-eol
    ;  whitespace = fix,space-before-tab,tab-in-indent,trailing-space
    ;  pager = less -+FRSX -FRX
    ;  excludesfile = ~/.gitignore_global
[alias]
    g = grep -I
    st = status
    ci = commit
    br = branch
    co = checkout
    sb = show-branch
    lg = log --stat
    last = log --stat -1
    diffstat = diff --stat -r
    changes = diff --name-status -r
    unstage = reset HEAD --
[merge]
    conflictstyle = diff3
    tool = vimdiff
    log = true
[pager]
    color = true
[status]
    color = auto
